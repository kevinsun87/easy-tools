{
   "action": {
      "default_icon": {
         "128": "icon/icon_128.png",
         "32": "icon/icon_32.png",
         "48": "icon/icon_48.png"
      },
      "default_popup": "dist/popup.html",
      "default_title": "iTab新标签页"
   },
   "background": {
      "service_worker": "service_worker.js"
   },
