      "128": "icons/icon_128.png",
      "32": "icons/icon_32.png",
      "48": "icons/icon_48.png",
      "64": "icons/icon_64.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Measure-it",
   "options_ui": {
      "browser_style": true,
      "page": "options.html"
   },
   "permissions": [ "activeTab", "storage", "scripting" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.0.0"
}
